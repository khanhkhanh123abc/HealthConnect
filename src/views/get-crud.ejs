<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Get Crud</title>
</head>

<body>
    <table border="1">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Address</th>
            <th>Phone Number</th <th>Gender</th>
            <th>Image</th>
            <th>Role ID</th>
        </tr>
        <!-- Data rows will be populated here -->
        <tr>
            <% for(let i=0; i< users.length; i++) { %>
                <td>
                    <%= users[i].firstName %>
                </td>
                <td>
                    <%= users[i].lastName %>
                </td>
                <td>
                    <%= users[i].address %>
                </td>
                <td>
                    <%= users[i].phoneNumber %>
                </td>
                <td>
                    <%= users[i].gender %>
                </td>
        </tr>
    </table>
</body>

</html>